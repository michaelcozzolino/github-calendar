{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "compilerOptions": {
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable"
        ],
        "rootDir": "src",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "noEmit": false
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.d.ts",
        "src/**/*.tsx",
        "src/**/*.vue"
    ],
    "exclude": [
        "node_modules"
    ]
}
